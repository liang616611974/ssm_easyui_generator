<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>
		代码生成器配置文件: 
		1.会为所有的property生成property_dir属性,如pkg=com.company => pkg_dir=com/company
		2.可以引用环境变量: ${env.JAVA_HOME} or System.properties: ${user.home},property之间也可以相互引用
	</comment>
	
	<!-- 包的基础路径 ${basepackage}.model-->
	<entry key="basePackage">com.liangfeng.study</entry>
	<entry key="module">goods</entry>
	<entry key="corePackage">${basePackage}.core</entry>
	<!--<entry key="commonPackage">${basePackage}.common</entry>-->
	<entry key="baseObjPackage">${corePackage}.framework.base</entry>
	<entry key="baseModulePackage">${basePackage}.${module}</entry>
	<entry key="pojoPackage">${baseModulePackage}.model.auto.pojo</entry>
	<entry key="qoPackage">${baseModulePackage}.model.auto.qo</entry>
	<entry key="mapperPackage">${baseModulePackage}.mapper</entry>
	<entry key="servicePackage">${baseModulePackage}.service</entry>
	<entry key="controllerPackage">${baseModulePackage}.web.controller</entry>
	<entry key="requestPackage">${baseModulePackage}.web.request</entry>
	<entry key="responsePackage">${baseModulePackage}.web.response</entry>
	<entry key="commonColumn">create_time,create_user,modify_time,modify_user</entry>
	<entry key="webModule">goods</entry>



	<!-- jsp namespace: web/${namespace}/${className}/list.jsp -->
	<entry key="namespace">pages</entry>

	<entry key="outRoot">C:\Users\Administrator\Desktop\generator\temp</entry>
	
	<!-- 需要移除的表名前缀,使用逗号进行分隔多个前缀,示例值: t_,v_ -->
	<entry key="tableRemovePrefixes">a_</entry>
	
	<!-- oracle需要指定jdbc.schema,并且可能要大写，其它数据库忽略此项配置  -->
	<entry key="jdbc.schema">FSWCM70</entry>
	<entry key="jdbc.catalog"></entry>
	
	<!-- 数据库类型至java类型映射  -->
	<entry key="java_typemapping.java.lang.Boolean">Boolean</entry>
	<entry key="java_typemapping.java.lang.Byte">Byte</entry>
	<entry key="java_typemapping.java.lang.Short">Integer</entry>
	<entry key="java_typemapping.java.lang.Integer">Integer</entry>
	<entry key="java_typemapping.java.lang.Long">Long</entry>
	<entry key="java_typemapping.java.lang.String">String</entry>
	<entry key="java_typemapping.java.sql.Clob">String</entry>
	<entry key="java_typemapping.java.sql.Date">Date</entry>
	<entry key="java_typemapping.java.sql.Time">Time</entry>	
	<entry key="java_typemapping.java.sql.Timestamp">Timestamp</entry>
	<entry key="java_typemapping.java.math.BigDecimal">BigDecimal</entry>
	
	<!-- Mysql -->
	<entry key="jdbc.url">jdbc:mysql://127.0.0.1:3306/spring_cloud_demo?characterEncoding=UTF-8</entry>
	<entry key="jdbc.driver">com.mysql.jdbc.Driver</entry>
	<entry key="jdbc.username">root</entry>
	<entry key="jdbc.password">root</entry>
	
	
